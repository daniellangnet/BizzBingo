@using BizzBingo.Web.Infrastructure.Raven.Indexes
@model List<LatestResourceIndex.LatestResourceResult> 
@{
    Layout = "";   
}

@for (int i = 0; i < Model.Count; i++)
{
            var resource = Model[i];
            <div class="@if (i != 0)
                        {
                            <text>offset1</text>
                        } span4"><h3>@resource.TermTitle</h3><a href="@resource.Url">Loading...</a><p>@resource.Title</p></div>
}         


